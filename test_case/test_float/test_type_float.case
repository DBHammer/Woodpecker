##获取数据库连接
Connection conn = GET_CONN[master];
##获取SQL执行器
Statement stmt = GET_STAT[conn];
##获取预编译SQL执行器
PStatement pstmt1 = GET_PSTAT[conn; "insert into test_type_float (c1, c2, c3, c4)values (?,?,?,?);";Decimal,Decimal,double,double];
##创建表，添加数据
SQL["drop table if exists test_type_float"; stmt];
SQL["create table test_type_float (c1 decimal(65, 30),c2 numeric(65, 30),c3 float,c4 double);"; stmt];
##执行预编译SQL
PSQL[pstmt1; -99999999999999999999999999999999999.999999999999999999999999999999, -99999999999999999999999999999999999.999999999999999999999999999999, -3.402823466E+38, -1.7976931348623157E+308];
PSQL[pstmt1; 99999999999999999999999999999999999.999999999999999999999999999999, 99999999999999999999999999999999999.999999999999999999999999999999, 3.402823466E+38, 1.7976931348623157E+308];
PSQL[pstmt1; 0, 0, 0, 0];
##获取运行结果集
ResultSet rs0 = SQL["select * from test_type_float"; stmt];
##指定理想结果集
IdealResultSet<double,double,double,double> irs0;
##导入该理想结果集
IMPORT_IRS["test_type_float"; irs0 0];
##比较数据库运行结果集与理想结果集
